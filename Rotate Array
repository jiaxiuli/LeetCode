class Solution {
    public void rotate(int[] nums, int k) {
     //   int nums[] = new int[]{1,2,3,4,5,6,7,8,9};
	//	int k =3 ;
		int temp;
		for( int i = 0 ; i < k ; i++) {
			temp = nums[nums.length-1];
			for( int j = nums.length-1 ; j > 0 ; j--) {
				nums[j] = nums[j-1];
			}
			nums[0] = temp;
			
		}
		
		
    }
}
