class Solution {
    public int balancedStringSplit(String s) {
       
		int R = 0, L = 0;
		int part = 0;
		
		for(int i = 0 ; i < s.length() ; i++) {
		if(s.charAt(i) == 'R') {
			R++;
		}else {
			L++;
		}
		if( R == L) {
			part++;
			R = 0;
			L = 0;
		}
		
		
		
		
	}
		
	
			
			
			return part;
			
		
    }
}
